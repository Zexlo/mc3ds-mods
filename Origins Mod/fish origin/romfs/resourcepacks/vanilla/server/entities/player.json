{
  "minecraft:entity": {
    "format_version": "1.1.0",

    "components": {
      "minecraft:identifier": {
        "id": "minecraft:player"
      },
      "minecraft:type_family": {
        "family": [ "player" ]
      },
      "minecraft:loot": {
        "table": "loot_tables/empty.json"
      },
      "minecraft:collision_box": {
        "width": 0.6,
        "height": 1.8
      },
      "minecraft:can_climb": {
      },
      "minecraft:behavior.guardian_attack": {
        "priority": 4
      },      
      "minecraft:movement": {
        "value": 0.7
      },
      "minecraft:health": {
        "value": 20,
        "max": 20
      },      
      "minecraft:attack": {
        "damage": 2
      },
      "minecraft:player.saturation": {
        "value": 20 
      },
      "minecraft:player.exhaustion": {
        "value": 0,
        "max": 4
      },
      "minecraft:player.level": {
        "value": 0,
        "max": 24791
      },
      "minecraft:player.experience": {
        "value": 0,
        "max": 1
      },
      "minecraft:breathable": {
        "breathesAir": false,
        "breathesWater": true,        
        "totalSupply":60,
        "suffocateTime":0,
        "generatesBubbles": true            
      },
      "minecraft:nameable": {
        "alwaysShow": true,
        "allowNameTagRenaming": false
      }

    }

  }
}
